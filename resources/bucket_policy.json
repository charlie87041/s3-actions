{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowUsersToAccessResourceSOME_USER",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::SOME_USER/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:ListBucketVersions"
      ],
      "Resource": [
        "arn:aws:s3:::SOME_USER/*"
      ]
    }
  ]
}